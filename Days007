
package pkg007;


public class Main {

    
    public static void main(String[] args) {
               
             //Tipe data karakter (char) dan tipe data boolean (boolean)
        
       // Tipe data karakter(char)yang dimama tipe datanya itu menggunakan tanda petik satu('')untuk menampilkan satu karater tunggal.
       //contoh
       
       char Gender = 'L';
        System.out.println("jenis kelamin = " + Gender );
        
        // Tipe data boolean(boolean)yang dimana tipe datanya itu cuman menampilkan true/false.
        //contoh
        
        boolean sehat = true;
        System.out.println("apakah badan saya sehat = "+ sehat );
    }
    
}
